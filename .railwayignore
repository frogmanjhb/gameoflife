server/node_modules/
client/node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json
server/package-lock.json
client/package-lock.json

# Build outputs (will be rebuilt)
dist/
build/
.vite/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db

# Logs
logs/
*.log

# Testing
coverage/
.nyc_output/

# Temporary files
tmp/
temp/
*.tmp

# Git
.git/
.gitignore

# Documentation (not needed for runtime)
*.md
!README.md
images/

